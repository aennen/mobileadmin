<!DOCTYPE html>
<html>
<head>
    <% include ../partials/head %>
    <title><%= title %></title>
    <link rel='stylesheet' href='./stylesheets/style.css'/>
    <link rel='stylesheet' href='./stylesheets/foundation.css'/>
</head>
<body>

<header>
    <% include ../partials/header %>
</header>

<h1><%= title %></h1>

<form action="rejectUser" method="POST">
    <div style="margin-left:10px;padding-left: 10px">

        <div class="scrollBox">
            <ul>
                <% if(payload != "") { %>
                <% payload.forEach(function(pay) { %>
                <ul><input name="<%= pay.login %>" id="<%= pay.login %>" type="checkbox" value="1"></input><label for="<%= pay.login %>"></label><%= pay.login %></ul>
                <% }); } %>

            </ul>
        </div>

        <p></p>

        <!-- use button tag for on page actions -->
        <p align="left">

            <button type="submit" class="button secondary" name="delete" value="1"><h5>Delete</h5></button>
            <button type="submit" class="button secondary" name="reset" value="1"><h5>Reset</h5></button>
        </p>
    </div>
    </div>

</form>

</body>

<footer>
    <% include ../partials/footer %>
</footer>
</html>
